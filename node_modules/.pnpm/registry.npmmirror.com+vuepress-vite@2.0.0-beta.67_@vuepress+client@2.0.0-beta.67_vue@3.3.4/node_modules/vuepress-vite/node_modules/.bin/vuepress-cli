#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/vuepress-starter/node_modules/.pnpm/registry.npmmirror.com+@vuepress+cli@2.0.0-beta.67/node_modules/@vuepress/cli/bin/node_modules:/mnt/c/vuepress-starter/node_modules/.pnpm/registry.npmmirror.com+@vuepress+cli@2.0.0-beta.67/node_modules/@vuepress/cli/node_modules:/mnt/c/vuepress-starter/node_modules/.pnpm/registry.npmmirror.com+@vuepress+cli@2.0.0-beta.67/node_modules/@vuepress/node_modules:/mnt/c/vuepress-starter/node_modules/.pnpm/registry.npmmirror.com+@vuepress+cli@2.0.0-beta.67/node_modules:/mnt/c/vuepress-starter/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/vuepress-starter/node_modules/.pnpm/registry.npmmirror.com+@vuepress+cli@2.0.0-beta.67/node_modules/@vuepress/cli/bin/node_modules:/mnt/c/vuepress-starter/node_modules/.pnpm/registry.npmmirror.com+@vuepress+cli@2.0.0-beta.67/node_modules/@vuepress/cli/node_modules:/mnt/c/vuepress-starter/node_modules/.pnpm/registry.npmmirror.com+@vuepress+cli@2.0.0-beta.67/node_modules/@vuepress/node_modules:/mnt/c/vuepress-starter/node_modules/.pnpm/registry.npmmirror.com+@vuepress+cli@2.0.0-beta.67/node_modules:/mnt/c/vuepress-starter/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../registry.npmmirror.com+@vuepress+cli@2.0.0-beta.67/node_modules/@vuepress/cli/bin/vuepress.js" "$@"
else
  exec node  "$basedir/../../../../../registry.npmmirror.com+@vuepress+cli@2.0.0-beta.67/node_modules/@vuepress/cli/bin/vuepress.js" "$@"
fi
